#include "libft.h"

char	**ft_resize_tab(char **str)
{
	int i;
	int j;
	int	j
		j = 0;
	i = 0;
	while(str[i])
	{
		j = ft_strlen(str[i]) - 1;
		if (str[i][j] == '.')
		{
			while (j && str[i][j] == '.')
			{

			}
		}

		i++;
	}
}
